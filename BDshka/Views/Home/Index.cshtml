@model IEnumerable<BDshka.Models.ClientsModel>

<h2>Список пользователей</h2>
<p><a asp-action="Registration">Добавить пользователя</a></p>
<p><a asp-action="Remont">Услуги</a></p>
<p><a asp-action="Worker">Работники</a></p>
<table class="table">
    <tr><th>ID</th><th>Имя</th><th>Телефон</th><th>Роль</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID_Client</td>
            <td>@item.FIO</td>
            <td>@item.Phone_Number</td>
            <td>@item.ID_Role</td>
        </tr>
    }
</table>
