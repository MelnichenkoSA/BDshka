@model IEnumerable<BDshka.Models.WorkersModel>

<h2>Список пользователей</h2>
<p><a asp-action="Registration">Добавить Работника</a></p>
<table class="table">
    <tr><th>ID</th><th>Имя</th><th>Оклад</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID_Worker</td>
            <td>@item.FIO</td>
            <td>@item.Oklad</td>
        </tr>
    }
</table>